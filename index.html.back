---
layout: default
---
<!-- ******HEADER****** -->
<header id="header" class="header">
  <div class="container">
    <h1 class="logo pull-left">
      <a href="{{ site.base_address }}">
        <span class="logo-title">{{ site.org_site_title }}</span>
      </a>
    </h1><!--//logo-->
    <nav id="main-nav" class="main-nav navbar-right" role="navigation">
      <div class="navbar-header">
        <button class="navbar-toggle" type="button" data-toggle="collapse" data-target="#navbar-collapse">
          <span class="sr-only">Open menu</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button><!--//nav-toggle-->
      </div><!--//navbar-header-->
      <div class="navbar-collapse collapse" id="navbar-collapse">
        <ul class="nav navbar-nav">
          <li class="active nav-item sr-only"><a class="scrollto" href="#promo">Home</a></li>
          <!--<li class="nav-item"><a class="scrollto" href="#about">About</a></li>-->
          <!--<li class="nav-item"><a class="scrollto" href="#join">Join Us</a></li>-->
          <li class="nav-item last"><a class="scrollto highlight" href="#donate">Donate</a></li>
          <li class="nav-item"><a class="scrollto" href="#contact">Contact</a></li>
        </ul><!--//nav-->
      </div><!--//navabr-collapse-->
    </nav><!--//main-nav-->
  </div>
</header><!--//header-->

<!-- ******PROMO****** -->
<section id="promo" class="promo section offset-header">
  <div class="container text-center">
    <h2 class="title h2-responsive slideInLeft top-title"><span class="highlight">{{ site.org_site_title }}</span></h2>
    <div class="center"><img src="assets/img/qr-code.png" alt="Donate for Ukraine"></div>
    <h2 class="title h2-responsive slideInRight bottom-title"><span class="highlight">{{ site.org_site_title_second}}</span></h2>
    <p class="intro">{{ site.org_summary }}</p>
    <div class="btns">
      <!--a class="btn btn-cta-secondary" href="#" target="_blank">Demo</a-->
      <a class="btn btn-outline btn-outline-medium scrollto" href="#donate" target="_blank">Donate <i class="fa fa-angle-double-down"></i></a>
    </div>
    <ul class="meta list-inline">
      <li></li>
    </ul><!--//meta-->
  </div><!--//container-->
  <div class="social-media">
    <div class="info text-center">
      <ul class="social-icons list-inline">
{% comment %}
        {% if site.org_twitter_username %}
        <li><a href="https://twitter.com/{{ site.twitter_username }}" target="_blank"><i class="fa fa-twitter"></i></a></li>
        {% endif %}
        {% if site.org_facebook_username %}
        <li><a href="https://www.facebook.com/{{ site.org_facebook_username }}" target="_blank"><i class="fa fa-facebook"></i></a></li>
        {% endif %}
        {% if site.org_instagram_username %}
        <li><a href="http://instagram.com/{{ site.instagram_username }}"><i class="fa fa-instagram"></i></a></li>
        {% endif %}
        {% if site.org_general_email_inbox %}
        <li class="last"><a href="mailto: {{ site.org_general_email_inbox }}"><i class="fa fa-envelope"></i></a></li>
        {% endif %}
{% endcomment %}
      </ul>
    </div>
  </div>
</section><!--//promo-->

