<!-- ******DONATE****** -->
<section id="donate" class="donate section text-center">
  <div class="container">
    <div class="donate-inner">
      <h2 class="title text-center">Donations for Ukraine</h2>
      <div class="info">
        <!--
        <p><strong>Become a donor today.</strong><br>As a small grassroots organization, know that every dollar will have an immense impact on our ability to continue working towards our mission.</p>
        -->
        <br>
        <br>
        <h4 class="text-center form-heading">Please choose an amount</h4>
        <div class="cta-container">
          <div class="speech-bubble">
            <p class="intro"></p>
            <div class="icon-holder text-center"><i class="fa fa-smile-o"></i></div>
          </div><!--//speech-bubble-->
        </div><!--//cta-container-->
        <form class="amount-chooser" target="_blank">
            <label><input type="radio" name="amount" class="checkbutton" value="10.00"><span>€10.00</span></label>
            <label><input type="radio" name="amount" class="checkbutton" value="20.00" checked="true"><span>€20.00</span></label>
            <label><input type="radio" name="amount" class="checkbutton" value="30.00"><span>€30.00</span></label>
            <br>
            <label><input type="radio" name="amount" class="checkbutton" value="50.00"><span>€50.00</span></label>
            <input type="number" class="textBox" name="amount" placeholder="€ anderen Betrag">
            <input type="hidden" name="currency" value="EUR">
            <br style="clear: both;"/>
        </form>
        <div class="btn-container  text-center" id="paypal-button-container">
        </div><!--//btn-container-->
        <div class="btn-container text-center">
          <h2>Payment details</h2>
        </div>
        <div class="text-center">
          <p>Empfänger: <strong>Botschaft der Ukraine in der Republik Österreich</strong></p>
          <p>IBAN: <strong>AT931200010036060043</strong></p>
          <p>BIC: <strong>BKAUATWW</strong></p>
          <br>
          <p>Verwendungszweck: <strong>Unterstützung der Ukraine</p>
          <br>
          <a href="{{ site.org_main_website_address }}"><img src="assets/img/embassy_logo.jpg" class="img-fluid rounded mx-auto d-block" style="width: 300px;"></a>
        </div>
      </div><!--//info-->
    </div><!--//donate-inner-->
  </div><!--//container-->
</section><!--//how-->

<!-- ******CONTACT****** -->
<section id="contact" class="contact section has-pattern">
  <div class="container">
    <div class="contact-inner">
      <h2 class="title  text-center">Contact</h2>
      <div class="text-center"><a href="{{ site.org_main_website_address }}"><img src="assets/img/embassy_logo.jpg" class="img-fluid rounded mx-auto d-block"></a></div>
      <address class="text-center">
        <strong>{{ site.org_name }}</strong><br>
        {% if site.org_street_address %}{{ site.org_street_address }},&nbsp;{% endif %}{% if site.org_address_suite %}{{ site.org_address_suite }}<br>{% endif %}
        {% if site.org_city %}{{ site.org_city }},&nbsp;{% endif %}{% if site.org_state %}{{ site.org_state }}&nbsp;{% endif %}{% if site.org_zip %}{{ site.org_zip }}{% endif %}<br>
        {% if site.org_phone %}
        <abbr title="Phone">Tel:</abbr> {{ site.org_phone }}
        {% endif %}<br>
        {% if site.org_main_website_address %}
        <a href="{{ site.org_main_website_address }}" target="_blank">{{ site.org_main_website_address }}</a>
        {% endif %}
      </address>
      <div class="clearfix"></div>
      <div class="info text-center">
        <!--<h4 class="sub-title">Unsere Verbindung</h4>-->
        <ul class="social-icons list-inline">
        {% if site.twitter_username %}
        <li><a href="https://twitter.com/{{ site.org_twitter_username }}" target="_blank"><i class="fa fa-twitter"></i></a></li>
        {% endif %}
        {% if site.org_facebook_username %}
        <li><a href="https://www.facebook.com/{{ site.org_facebook_username }}" target="_blank"><i class="fa fa-facebook"></i></a></li>
        {% endif %}
        {% if site.instagram_username %}
        <li><a href="http://instagram.com/{{ site.org_instagram_username }}" target="_blank"><i class="fa fa-instagram"></i></a></li>
        {% endif %}
        {% if site.org_general_email_inbox %}
        <li class="last"><a href="mailto: {{ site.org_general_email_inbox }}"><i class="fa fa-envelope"></i></a></li>
        {% endif %}
      </ul>
      </div><!--//info-->
    </div><!--//contact-inner-->
  </div><!--//container-->
</section><!--//contact-->
